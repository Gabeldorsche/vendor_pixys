# Build fingerprint
ifeq ($(BUILD_FINGERPRINT),)
BUILD_NUMBER_CUSTOM := $(shell date -u +%H%M)
CUSTOM_DEVICE ?= $(TARGET_DEVICE)
BUILD_SIGNATURE_KEYS := release-keys
CUSTOM_BUILD_VARIANT ?= $(TARGET_BUILD_VARIANT)
PIXYS_BUILD_FINGERPRINT := $(PRODUCT_BRAND)/$(CUSTOM_DEVICE)/$(CUSTOM_DEVICE):$(PLATFORM_VERSION)/$(BUILD_ID)/$(BUILD_NUMBER_CUSTOM):$(CUSTOM_BUILD_VARIANT)/$(BUILD_SIGNATURE_KEYS)

PRODUCT_BUILD_PROP_OVERRIDES += \
	BuildFingerprint=$(PIXYS_BUILD_FINGERPRINT)

endif
